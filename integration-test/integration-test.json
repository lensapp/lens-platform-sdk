{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "moduleDirectories": [
    "node_modules"
  ],
  "rootDir": ".",
  "testRegex": ".*\\.test\\.ts$",
  "transform": {
    "\\.[jt]sx?$": [
      "babel-jest",
      {
        "presets": [
          "@babel/preset-env",
          "@babel/preset-react",
          "@babel/preset-typescript"
        ],
        "plugins": [
          "@babel/plugin-transform-runtime",
          "@babel/plugin-proposal-class-properties"
        ]
      }
    ]
  },
  "testEnvironment": "node"
}
